<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:control="clr-namespace:WYW.UI.Controls">
    <Style TargetType="control:CircleProgressBar">
        <Setter Property="Maximum" Value="100"/>
        <Setter Property="RingThickness" Value="10.0"/>
        <Setter Property="MinHeight" Value="50.0"/>
        <Setter Property="MinWidth" Value="50.0"/>
        <Setter Property="Foreground" Value="{StaticResource Item.BackgroundSelected}"/>
        <Setter Property="RingBackground" Value="{StaticResource Ticker.Background}"/>
    </Style>
</ResourceDictionary>